<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <data noupdate="1">
        <record id="sequence_hospital_patient" model="ir.sequence">
            <field name="name">Patient Sequence</field>
            <field name="code">hospital.patient.sequence</field>
            <field name="prefix">HP</field>
            <field name="padding">3</field>
            <field name="company_id" eval="False"/>

        </record>
        <record id="sequence_hospital_appointment" model="ir.sequence">
            <field name="name">Patient Appoitment</field>
            <field name="code">hospital.appointment.sequence</field>
            <field name="prefix">HA</field>
            <field name="padding">3</field>
            <field name="company_id" eval="False"/>
        </record>
        <record id="sequence_doctor" model="ir.sequence">
            <field name="name">Doctors Sequence</field>
            <field name="code">doctors.sequence</field>
            <field name="prefix">DR</field>
            <field name="padding">3</field>
            <field name="company_id" eval="False"/>
        </record>

    </data>
</odoo>